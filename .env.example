# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/leiturinha

# JWT Configuration
JWT_SECRET=your-jwt-secret-key-here
JWT_EXPIRATION=7d

# AI Service Providers
OPENAI_API_KEY=your-openai-key-here
ANTHROPIC_API_KEY=your-anthropic-key-here
STABILITY_API_KEY=your-stability-key-here
REPLICATE_API_KEY=your-replicate-key-here
GETIMG_API_KEY=your-getimg-key-here
RUNWARE_API_KEY=your-runware-key-here
LEXICA_API_KEY=your-lexica-key-here

# App Configuration
NODE_ENV=development
PORT=3000
SESSION_SECRET=your-session-secret-here
COOKIE_SECRET=your-cookie-secret-here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=debug

# Cache Settings
CACHE_TTL=3600